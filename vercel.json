{
  "version": 2,
  "github": {
    "silent": true
  },
  "builds": [
    {
      "src": "nuxt.config.js",
      "use": "@nuxtjs/now-builder",
      "config": {
        "serverFiles": ["api/**"]
      }
    }
  ],
  "routes": [
    {
      "src": "/api/contents/[id]",
      "dest": "/api/contents/[id].ts",
      "methods": ["GET", "OPTIONS"],
      "headers": {
        "Access-Control-Allow-Methods": "GET",
        "Access-Control-Allow-Headers": "Content-Type"
      }
    },
    {
      "src": "/api/hello",
      "dest": "/api/hello.ts",
      "methods": ["GET", "OPTIONS"],
      "headers": {
        "Access-Control-Allow-Methods": "GET",
        "Access-Control-Allow-Headers": "Content-Type"
      }
    }
  ]
}
